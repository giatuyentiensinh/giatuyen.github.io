{
  "manifest_version": 2,

  "name": "Extension",
  "description": "Gia tuyen tien sinh web.",
  "version": "1.0",

  "browser_action": {
    "default_title": "Tuyen's website",
    "default_icon": "assets/images/itbee.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab"
  ],

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
